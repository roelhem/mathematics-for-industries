extra-source-files: CHANGELOG.md

library
    exposed-modules: Lib, Common, Types, DP
    build-depends:    base ^>=4.14.3.0
                    , lens
                    , generic-lens
                    , prettyprinter
                    , data-default
                    , comonad
    hs-source-dirs:   src
    default-language: Haskell2010
